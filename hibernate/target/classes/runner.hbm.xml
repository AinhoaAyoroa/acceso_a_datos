<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
  "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.mycompany.hibernate.Tablas.runner" table="Runner">
        <id name="idCorredor" type="int">
            <generator class="native"/>
        </id>
        <property name="dni" type="string" />
        <property name="nom" />
        <property name="cognoms" />
        <property name="edat" />
        <property name="dorsal" />
        <property name="categoria" />
        <property name="localitat" />
        <property name="email" />
    </class>
</hibernate-mapping>
